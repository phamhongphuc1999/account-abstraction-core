  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./eth-errors": {
      "import": "./dist/esm/eth-errors/index.js",
      "require": "./dist/cjs/eth-errors/index.js"
    },
    "./account": {
      "import": "./dist/esm/account.js",
      "require": "./dist/cjs/account.js"
    },
    "./constants": {
      "import": "./dist/esm/constants.js",
      "require": "./dist/cjs/constants.js"
    },
    "./error": {
      "import": "./dist/esm/error.js",
      "require": "./dist/cjs/error.js"
    },
    "./eth": {
      "import": "./dist/esm/eth.js",
      "require": "./dist/cjs/eth.js"
    },
    "./keyring": {
      "import": "./dist/esm/keyring.js",
      "require": "./dist/cjs/keyring.js"
    },
    "./query": {
      "import": "./dist/esm/query.js",
