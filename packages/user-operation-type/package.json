    },
    "./user-operation": {
      "import": "./dist/esm/user-operation.js",
      "require": "./dist/cjs/user-operation.js"
    },
    "./utils": {
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.js"
    }
